<% layout('layouts/boilerplate')%>


<div class="row">
    <h1 class="text-center">New Data</h1>
    <div class="col-6 offset-3">
        <form action="/datas" method="POST" enctype="multipart/form-data" novalidate class="validated-form">
            <div class="mb-3">
                <label for="formFile" class="form-label">Choose File</label>
                <input class="form-control" type="file" id="formFile" name="csvFile">
                <div class="valid-feedback">
                    Looks good!
                </div>
            </div> 
            <div class="mb-3">
                <button class="btn btn-success">Add Data</button>
            </div>
        </form>
    </div>
</div>